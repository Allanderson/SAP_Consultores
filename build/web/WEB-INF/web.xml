    <servlet>
        <servlet-name>CadastrarProduto</servlet-name>
        <servlet-class>servlet.CadastrarProduto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoverProduto</servlet-name>
        <servlet-class>servlet.RemoverProduto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoverClientes</servlet-name>
        <servlet-class>servlet.RemoverClientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AtualizarDadosCliente</servlet-name>
        <servlet-class>servlet.AtualizarDadosCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logoff</servlet-name>
        <servlet-class>servlet.Logoff</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SapConsultores</servlet-name>
        <url-pattern>/SapConsultores</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>conexaoBanco</servlet-name>
        <url-pattern>/conexaoBanco</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroCliente</servlet-name>
        <url-pattern>/CadastroCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginCliente</servlet-name>
        <url-pattern>/LoginCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarAdministrador</servlet-name>
        <url-pattern>/CadastrarAdministrador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginAdministrador</servlet-name>
        <url-pattern>/LoginAdministrador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DarBaixa</servlet-name>
        <url-pattern>/DarBaixa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VendaProdutos</servlet-name>
        <url-pattern>/VendaProdutos</url-pattern>
