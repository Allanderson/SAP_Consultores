<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>SapConsultores</servlet-name>
        <servlet-class>SapConsultores</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>conexaoBanco</servlet-name>
        <servlet-class>servlet.conexaoBanco</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastroCliente</servlet-name>
        <servlet-class>servlet.CadastroCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginCliente</servlet-name>
        <servlet-class>servlet.LoginCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarAdministrador</servlet-name>
        <servlet-class>servlet.CadastrarAdministrador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginAdministrador</servlet-name>
        <servlet-class>servlet.LoginAdministrador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DarBaixa</servlet-name>
        <servlet-class>servlet.DarBaixa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VendaProdutos</servlet-name>
